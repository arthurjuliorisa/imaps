{
  "name": "imaps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "seed": "npx prisma db seed",
    "seed:menu": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/seed-menus.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:validate": "prisma validate",
    "db:format": "prisma format",
    "db:reset-seed": "npm run db:migrate:reset -- --force && npm run db:seed",
    "db:setup-partitions": "psql -d imaps_db -f setup_partitions.sql",
    "db:create-views": "psql -d imaps_db -f create_views.sql",
    "db:setup-complete": "npm run db:setup-partitions && npm run db:create-views",
    "type-check": "tsc --project tsconfig.build.json --noEmit",
    "type-check:all": "tsc --noEmit",
    "db:setup": "tsx scripts/setup-db.ts",
    "db:verify": "tsx scripts/verify-db.ts",
    "db:dev": "npm run db:setup && npm run db:seed",
    "vercel-build": "prisma generate && next build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-date-pickers": "^8.17.0",
    "@prisma/client": "^6.19.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "next": "^16.0.1",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.67.0",
    "validator": "^13.15.22",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/validator": "^13.15.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.8",
    "postcss": "^8.4.49",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
